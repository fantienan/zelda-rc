{
  "name": "zelda-rc",
  "version": "0.3.2",
  "private": false,
  "main": "dist/es/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "license": "MIT",
  "description": "React components library",
  "author": "fantienan",
  "keywords": [
    "Component",
    "UI",
    "React",
    "storybook",
    "ant design"
  ],
  "homepage": "https://fantienan.github.io/zelda-rc",
  "repository": {
    "type": "git",
    "url": "https://github.com/fantienan/zelda-rc"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "antd": ">=4.2.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@types/react-window": "^1.8.2",
    "react-window": "^1.8.5",
    "react-rnd": "^10.2.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "craco start",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "sb": "start-storybook -p 6006 -s public",
    "sb:debug": "npm run storybook --debug-webpack",
    "sb:build": "rimraf ./storybook-static build-storybook -s public",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext js,ts,tsx src/components --max-warnings 5",
    "build:ts": "tsc -p tsconfig.build.json",
    "build:less": "cd scripts && gulp",
    "build": "npm run clean && npm run build:ts && npm run build:less",
    "prepublishOnly": "npm run test:nowatch && npm run lint && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@craco/craco": "^5.6.4",
    "@storybook/addon-actions": "^6.0.10",
    "@storybook/addon-essentials": "^6.0.10",
    "@storybook/addon-links": "^6.0.10",
    "@storybook/node-logger": "^6.0.10",
    "@storybook/preset-ant-design": "^0.0.1",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-resizable": "^1.7.2",
    "antd": "^4.5.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "craco-less": "^1.17.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^4.0.1",
    "husky": "^4.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-resizable": "^1.10.1",
    "react-scripts": "3.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "~3.7.2",
    "eslint": "^6.6.0",
    "rc-resize-observer": "^0.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
